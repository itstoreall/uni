{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "rewrites": [
    { "source": "/socket.io/:path*", "destination": "/api/socket.io/:path*" },
    { "source": "/(.*)", "destination": "dist/index.js" }
  ]
}
